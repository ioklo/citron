// template namespace 
// namespace 이름<다른 namedObject>
// namedObject: 이름 붙일 수 있는 것들.. class, namespace 
// namespace는 기본이 public, 감추고 싶은 것들만 private
// static class라고 생각해도 될 것 같다.

namespace A<T>
{
    int VirtualFunc()
    {
        T.Func();
    }
}

namespace Test.P1
{
    // there's a function 
    int Func()
    {
        return 1;
    }
}

namespace Test.P2
{
    int Func()
    {
        return 2;
    }
}

// 쓸때는 
int res1 = A<P1>.Func();
int res2 = A<P2>.Func();