
// 주석달기
bool test()
{
    int sum1 = 0;
    for (int t = 0; t < 10; t++)
	{
        sum1 += t;
	}

	int sum2 = 0;
	for (int t = 0; t < 10; t++)	
	{
		if (t < 5) continue;
		sum2 += t;
	}

	int sum3 = 0;
	for (int t = 0; t < 10; t++)
	{
		if (t > 5) break;
		
        sum3 += t;
	}

    return (sum1 == 45) && (sum2 == 35) && (sum3 == 15);
}
